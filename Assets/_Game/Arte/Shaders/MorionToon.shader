			float dotResult4 = dot( ase_objectlightDir , ase_vertexNormal );
			float temp_output_7_0 = saturate( dotResult4 );
			o.Emission = ( tex2D( _Fondo, uv_Fondo ) * SampleGradient( gradient8, temp_output_7_0 ) ).rgb;
